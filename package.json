{
  "name": "vite-plugin-md2vue",
  "version": "0.0.2",
  "description": "vite plugin to transform  markdown to vue",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc -w -p . --skipLibCheck",
    "test": "jest --clearCache && jest --runInBand --forceExit",
    "build": "rm -rf dist && tsc -p . --skipLibCheck",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublishOnly": "yarn build && yarn changelog",
    "postpublish": "git add CHANGELOG.md && git commit -m 'chore: changelog [ci skip]'"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vite",
    "md"
  ],
  "repository": "https://github.com/WangXueZhi/vite-plugin-md2vue.git",
  "author": "wangxuezhi",
  "license": "MIT",
  "dependencies": {
    "highlight.js": "^10.7.2",
    "marked": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "conventional-changelog-cli": "^2.1.1",
    "typescript": "^4.2.3"
  }
}
